mixin meetpartner(data)
  .meet-partner
    h1 #{data.title}
    .meet-pattner__content
      mixin person(classIndex, imgIndex, nameIndex, textIndex)
        .meet-partner__item
           div(class="meet-partner__avatar-block " + data.myclass[classIndex])
             img(src="" + data.myimages[imgIndex] width="120px" height="120px" alt="persons")
           h3 #{data.name[nameIndex]}
           p #{data.text[textIndex]}
      +person(0,0,0,0)
      +person(1,1,1,1)
      +person(2,2,2,2)
      +person(3,3,3,3)
    button.meet-partner__btn #{data.btn}
